<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="NoImagePanel">
		<bg>wxSYS_COLOUR_INACTIVECAPTION</bg>
		<id_arg>0</id_arg>
		<object class="wxGridSizer" variable="GridSizer1" member="no">
			<cols>1</cols>
			<rows>1</rows>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer1" member="no">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="imageNumTxt" member="yes">
							<label>1</label>
							<fg>wxSYS_COLOUR_HIGHLIGHT</fg>
							<font>
								<weight>bold</weight>
								<sysfont>wxSYS_DEFAULT_GUI_FONT</sysfont>
								<relativesize>8.000000</relativesize>
							</font>
							<style>wxST_NO_AUTORESIZE|wxALIGN_CENTRE</style>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>click capture to take a picture</label>
							<fg>wxSYS_COLOUR_INACTIVECAPTIONTEXT</fg>
							<font>
								<weight>bold</weight>
								<sysfont>wxSYS_DEFAULT_GUI_FONT</sysfont>
								<relativesize>1.000000</relativesize>
							</font>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
